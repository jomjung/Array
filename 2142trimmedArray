#include <iostream>
using namespace std;
int main(int argc, const char * argv[]) {
    int t,n,m;
    cin >> t;
    string ans[t];
    for (int i=0; i<t; i++){
        bool isRight = true;
        cin >> n >> m;
        int arr[n][m];
        for (int j=0;j<n;j++) for (int k=0; k<m; k++) scanf("%d", &arr[j][k]);
        for (int j=0; j<n-1; j++)
            for (int k=0; k<m-1; k++) if ( arr[j][k] + arr[j+1][k+1] > arr[j+1][k] + arr[j][k+1] ) isRight = false;
        if (isRight) ans[i] = "YES"; else ans[i] = "NO";
    }
    for (int i=0; i<t; i++) cout << ans[i] << "\n";
    return 0;
}
